{
  "name": "any-progress",
  "version": "1.3.2",
  "description": "元素顶部快速开关进度条，适用移动端、PC端。",
  "main": "./dist/any-progress.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.production.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.dev.js",
    "start": "npm run dev",
    "ref": "node ./build/reference.js",
    "release": "npm run build && npm run ref"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiNISAL/any-progress.git"
  },
  "keywords": [
    "progress",
    "进度条"
  ],
  "author": "NISAL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiNISAL/any-progress/issues"
  },
  "homepage": "https://github.com/hiNISAL/any-progress#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-stage-0": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
